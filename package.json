{
  "name": "nestjs_boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config test/jest.json --runInBand --passWithNoTests --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node ./node_modules/typeorm/cli -d src/configs/database.config.ts",
    "typeorm:generate": "npm run typeorm migration:generate ./src/common/database/migrations/$npm_config_name",
    "typeorm:create": "ts-node ./node_modules/typeorm/cli migration:create ./src/common/database/migrations/$npm_config_name",
    "migration:run": "npm run typeorm migration:run",
    "migration:generate": "npm run build && npm run typeorm:generate",
    "migration:create": "npm run typeorm:create",
    "migration:revert": "npm run typeorm migration:revert",
    "prepare": "node .husky/install.mjs",
    "gen": "crudify $npm_config_name && npm run lint"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.2",
    "@nestjs/bullmq": "^10.1.1",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.3.8",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.8",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.8",
    "@nestjs/schedule": "^4.0.2",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/throttler": "^5.1.2",
    "@nestjs/typeorm": "^10.0.2",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "bullmq": "^5.7.4",
    "cache-manager": "^5.5.1",
    "cache-manager-redis-yet": "^5.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "mysql2": "^3.9.7",
    "nest-winston": "^1.9.4",
    "nestjs-i18n": "^10.4.5",
    "nodemailer": "^6.9.13",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.1",
    "pretty-error": "^4.0.0",
    "reflect-metadata": "^0.2.2",
    "rotating-file-stream": "^3.2.1",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.0",
    "@commitlint/config-conventional": "^19.1.0",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.8",
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.12",
    "@types/morgan": "^1.9.9",
    "@types/ms": "^0.7.34",
    "@types/node": "20.12.7",
    "@types/nodemailer": "^6.4.14",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "husky": "^9.0.11",
    "jest": "29.7.0",
    "nest-crudify": "^1.0.6",
    "prettier": "^3.2.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.4.5"
  }
}
